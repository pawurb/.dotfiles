{
    "workbench.colorTheme": "RailsCasts Theme",
    "vim.normalModeKeyBindingsNonRecursive": [],
    "editor.minimap.enabled": false,
    "editor.minimap.autohide": true,
    "editor.tabSize": 2,
    "editor.detectIndentation": true,
    "extensions.autoUpdate": false,
    "extensions.autoCheckUpdates": false,
    "editor.formatOnSave": true,
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "k",
                "j"
            ],
            "after": [
                "<esc>"
            ]
        }
    ],
    "editor.fontSize": 13,
    "files.autoSave": "off",
    "files.autoSaveDelay": 5000,
    "git.enabled": false,
    "editor.wordWrap": "on",
    "typescript.validate.enable": false,
    "json.validate.enable": false,
    "javascript.validate.enable": false,
    "search.useIgnoreFiles": false,
    "vim.ignorecase": true,
    "vim.useSystemClipboard": false,
    "editor.fontFamily": "Hack Regular, Menlo, Monaco, 'Courier New', monospace",
    "extensions.ignoreRecommendations": true,
    "editor.occurrencesHighlight": "off",
    "rust-analyzer.rustfmt.extraArgs": [
        "+nightly"
    ],
    "editor.inlayHints.enabled": "offUnlessPressed",
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust-analyzer"
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/*.code-search": true,
        "**/target": true,
        "/tmp/**": true,
        "/tmp/git-blob**": true,
        ".jekyll-cache": true,
        "_blog_production": true,
        "_blog_development": true,
    },
    "workbench.colorCustomizations": {
        "tab.activeBorder": "#eee"
    },
    "editor.wordBasedSuggestions": "off",
    "editor.quickSuggestionsDelay": 500,
    "editor.formatOnPaste": true,
    "files.associations": {
        ".env-remote": "shellscript",
        "*.toml": "toml",
        ".env-remote-reth": "ignore"
    },
    "workbench.startupEditor": "none",
    "editor.quickSuggestions": {
        "other": "off",
        "comments": "off",
        "strings": "off"
    },
    "editor.hover.enabled": true,
    "rust-analyzer.completion.limit": 100,
    "rust-analyzer.lens.implementations.enable": false,
    "rust-analyzer.lens.debug.enable": false,
    "rust-analyzer.lens.run.enable": false,
    "editor.codeLens": false,
    "[html]": {
        "editor.formatOnSave": false,
        "editor.suggest.insertMode": "replace"
    },
    "workbench.activityBar.location": "hidden",
    "cursor.cpp.disabledLanguages": [
        "plaintext",
        "markdown",
        "scminput"
    ],
    "cursor.cmdk.autoSelect": false,
    "[toml]": {
        "editor.formatOnSave": false
    },
    "[proto3]": {
        "editor.formatOnSave": false
    },
    "rust-analyzer.check.overrideCommand": [
        "/Users/pablo/.cargo/bin/cargo",
        // "+nightly",
        "clippy",
        "--all",
        "--examples",
        "--tests",
        "--benches",
        // "--all-features",
        "--message-format=json"
    ],
    "workbench.statusBar.visible": false,
}